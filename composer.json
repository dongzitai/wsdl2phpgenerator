{
    "name": "dhcmediway/wsdl2phpgenerator",
    "description": "Simple class library for generating php classes from a wsdl file.",
    "keywords": ["soap", "wsdl"],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Wsdl2PhpGenerator\\": ["src/", "lib/"]
        }
    },
    "require": {
        "php": ">=5.3.0",
        "symfony/options-resolver": "~2.6|~3.0|~5.0",
        "symfony/polyfill-iconv": "^1.2"
    }
}
